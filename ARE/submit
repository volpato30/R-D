sqsub -q gpu -r 2d --mpp 200G -o train1_32.log python -u ARE_transposeCnn_linearLayer.py 1 32
sqsub -q gpu -r 2d --mpp 200G -o train2_64.log python -u ARE_transposeCnn_linearLayer.py 2 64
sqsub -q gpu -r 2d --mpp 200G -o train3_128.log python -u ARE_transposeCnn_linearLayer.py 3 128
sqsub -q gpu -r 2d --mpp 200G -o train4_256.log python -u ARE_transposeCnn_linearLayer.py 4 256
